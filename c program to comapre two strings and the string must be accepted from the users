#include<stdio.h>
#include<string.h>
int main(){
char s1[50],s2[50];
int result;
printf("Enter the first string:");
scanf("%s",s1);
printf("Enter the second string:");
scanf("%s",s2);
result=strcmp(s1,s2);
printf("Result:%d\n",result);
if(result==0)
{
    printf("string are equal");
}
else if(result>0)
{
    printf("\"%s\"string is greater\n",s1);
}
else
{
    printf("\"%s\"string is lesser\n",s2);
}
return 0;
}
