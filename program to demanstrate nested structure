#include <stdio.h>
struct marks{
int maths ;
int physics;
int linux;
};
struct student{
int id;
char name[20];
struct marks;//nested structure
};
int main(){
struct student s1={101,"Kiran",{89,86,57}};
printf("Name:%s\n",s1.name);
printf("marks:%d,%d,%d\n",s1.marks.maths,s1.marks.physics,s1.marks.linux);
return 0;
}
